{
    "version": "1.0.0-*",
    "testRunner": "xunit",
    "dependencies": {
        "dotnet-test-xunit": "2.2.0-preview2-build1029",
        "xunit": "2.2.0-beta5-build3474",
        "TestCenter.Api": {
            "target": "project"
        },
        "TestCenter.Services": {
            "target": "project"
        },
        "TestCenter.Models": {
            "target": "project"
        }
    },
    "frameworks": {
        "netcoreapp1.1": {
            "dependencies": {
                "Microsoft.NETCore.App": {
                    "type": "platform",
                    "version": "1.1.0"
                }
            }
        }
    },
    "buildOptions": {
        "copyToOutput": {
            "include": [
                "xunit.runner.json"
            ]
        }
    },
    "tools": {
        "Microsoft.DotNet.Watcher.Tools": "1.1.0-preview4-final"
    },
    "tooling": {
        "defaultNamespace": "TestCenter.UnitTests.Api"
    }
}